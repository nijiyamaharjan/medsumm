{
  "expo": {
    "name": "MedSumm",
    "slug": "MedSumm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/medsumm.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/medsumm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/medsumm.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/medsumm.png"
    },

    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }

}
